{
  "vars": {
    "frontBumper":"$bumper",
    "backBumper":"$bumper"
  },
  "args":[
    ["-i", "$frontBumper"],
    ["-i", "$input"],
    ["-i", "$backBumper"],
    ["-vn"],
    ["-filter_complex", "[0][1]acrossfade=d=10:c1=tri:c2=tri[a01]; [a01][2]acrossfade=d=10:c1=tri:c2=tri[a02]"],
    ["-f", "mp3", "$outFile.mp3"]
  ]
}
